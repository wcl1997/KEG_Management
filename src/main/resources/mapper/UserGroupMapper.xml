<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ysu.mapper.UserGroupMapper">

    <select id="list" resultType="com.ysu.entity.UserGroup">
        SELECT g_id,g_name  FROM user_group
    </select>
    <select id="getUserGroup" resultType="com.ysu.entity.UserGroup">
        SELECT g_id,g_name  FROM user_group WHERE g_id != 3
    </select>
</mapper>
